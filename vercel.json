{
  "version": 2,
  "builds": [
    {
      "src": "api/send-email.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "/api/send-email"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}